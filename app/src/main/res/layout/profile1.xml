        </LinearLayout>

        <LinearLayout
            android:id="@+id/p1_Button6"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/ic_profile1_round_button6"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/check6"
                android:layout_width="24dp"
                android:layout_height="21dp"
                android:layout_marginTop="11dp"
                android:layout_marginStart="16dp"
                android:alpha=".2"
                android:background="@drawable/ic_profile1_white_circle"
                android:gravity="center_vertical"
                android:orientation="horizontal">
            </LinearLayout>

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:fontFamily="@font/avenir_ltstd_book"
                    android:gravity="center_vertical"
                    android:textSize="17sp"
                    android:textColor="@color/white"
                    android:layout_marginStart="16dp"
                    android:text="Entertainment" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/p1_Button7"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_marginTop="8dp"
